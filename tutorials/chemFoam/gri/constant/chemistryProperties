FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver          ode_pyJac;
    method          loadBalanced_pyJac;
}

chemistry       on;

initialChemicalTimeStep 1e-7;

EulerImplicitCoeffs
{
    cTauChem        1;
    equilibriumRateLimiter off;
}

odeCoeffs
{
    solver          seulex_LAPACK;
    absTol          1e-8;
    relTol          1e-5;
}

loadbalancing{}
refmapping{}

#include "reactions"
